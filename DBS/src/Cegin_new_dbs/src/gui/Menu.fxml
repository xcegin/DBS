        <MenuItem fx:id="menuFind" mnemonicParsing="false" onAction="#menuAddAction" text="Pridanie a Nájdenie" />
            <MenuItem fx:id="menuDelete" mnemonicParsing="false" onAction="#menuDeleteAction" text="Odstránenie a Aktualizácia" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Štatistiky">
      <items>
        <MenuItem fx:id="menuStatistic" mnemonicParsing="false" onAction="#menuStatisticsAction" text="Zobraziť štatistiky" />
      </items>
    </Menu>
      <Menu mnemonicParsing="false" text="Zamestnanci">
        <items>
          <MenuItem fx:id="menuEmploy" mnemonicParsing="false" onAction="#menuEmployAction" text="Pridať a Priradiť" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Pracovne zaznamy">

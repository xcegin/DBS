      <Menu mnemonicParsing="false" text="Hľadanie">
      <items>
      <MenuItem fx:id="menuSearch" mnemonicParsing="false" onAction="#menuFindAction" text="Zamestnanci"/>
      </items>
        </Menu>
        
        <Menu mnemonicParsing="false" text="Traktory">
      <items>
      <MenuItem fx:id="menuTraktor" mnemonicParsing="false" onAction="#menuTraktorAction" text="Priradenie"/>
      </items>
        </Menu>
        
        
        <Menu mnemonicParsing="false" text="Koniec">
      <items>
      <MenuItem fx:id="menuKoniec" mnemonicParsing="false" onAction="#menuKoniecAction" text="Koniec"/>
      </items>
        </Menu>
        
  </menus>
</MenuBar>
